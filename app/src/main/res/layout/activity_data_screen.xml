<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/data_start_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:inputType="text"
        android:text="@string/data_start"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="8dp" />

    <TextView
        android:id="@+id/data_stop_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="@string/data_stop"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="66dp" />

    <TextView
        android:id="@+id/przebieg_start_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Przebieg start"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="126dp" />

    <TextView
        android:id="@+id/przebieg_stop_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Przebieg stop"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="162dp" />

    <TextView
        android:id="@+id/obrot_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Obrót"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="196dp" />

    <TextView
        android:id="@+id/mytaxi_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Mytaxi"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="266dp" />

    <TextView
        android:id="@+id/terminal_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Terminal"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="336dp" />

    <TextView
        android:id="@+id/firmowe_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="kursy firmowe"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="406dp" />

    <TextView
        android:id="@+id/paliwo_litry_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Paliwo litry"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="476dp" />

    <TextView
        android:id="@+id/paliwo_zl_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Paliwo zl"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="546dp" />

    <TextView
        android:id="@+id/sprzatanie_label"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="Sprzątanie"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="616dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:inputType="date"
        android:text="Data"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Data"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Godzina"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Godzina"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:inputType="text"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:layout_editor_absoluteX="158dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="150dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="Wstecz"
        app:layout_constraintBaseline_toBaselineOf="@+id/button11"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="Sprawdź"
        app:layout_constraintBaseline_toBaselineOf="@+id/button12"
        app:layout_constraintEnd_toStartOf="@+id/button12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button9" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="Zapisz"
        app:layout_constraintBaseline_toBaselineOf="@+id/button13"
        app:layout_constraintEnd_toStartOf="@+id/button13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button11" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="Dalej"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button12" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="246dp" />

</android.support.constraint.ConstraintLayout>